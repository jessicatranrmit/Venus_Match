<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.jessica.venus_match">

    <uses-permission android:name="android.permission.INTERNET"></uses-permission>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".view.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".view.Edit_Profile"
            android:label="@string/app_name" />
        <activity
            android:name=".view.ProfileActivity"
            android:label="@string/app_name">

        </activity>
        <activity android:name=".view.RegisterActivity" />
        <activity android:name=".view.DashboardActivity" />
        <activity android:name=".view.ActivationActivity" />
        <activity android:name=".view.ShowProfileActivity" />
    </application>

</manifest>